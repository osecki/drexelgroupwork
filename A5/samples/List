Class list(init)
       L := init;
       Cons := proc(x) L := cons(x, L); return := 0 end;
       Car := proc() return := car(L) end;
       Cdr := proc() return :=cdr(L) end
end;

       L := list([]);
       a := L.Cons(3);
       b := L.Cons(2);
       c := L.Cons(1);
       x := L.Car();
       M := L.Cdr()

