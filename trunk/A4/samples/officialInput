a := proc()
    return := proc()
        return := 10
    end
end;

b := a();
ten := b();


d := proc(x)
    return := x
end;

tenAlso := d(ten);

f := proc()
    g := proc ()
        return := 20
    end;

    return := g()
end;

twenty := f();

makeadder := proc(x)
    return := proc (y)
        return := x + y
    end
end;

addone := makeadder(1);

x := 2;

three := addone(2)
